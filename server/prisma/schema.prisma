// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
    provider = "postgres"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
    output   = "../src/.prismaClient"
}

model User{
  id Int @id @default(autoincrement())
  name String @unique
  password String
  type UserType

  @@unique([id,name])
}

enum UserType {
  USER
  ADMIN
}